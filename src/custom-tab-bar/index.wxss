.tab-bar {
  position: absolute;
  box-sizing: border-box;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: row;
  padding-top: 8rpx;
  padding-bottom: env(safe-area-inset-bottom);
  pointer-events: auto;
  border-top: 1rpx solid #eeeeee;
  border-top-left-radius: 48rpx;
  border-top-right-radius: 48rpx;
}
