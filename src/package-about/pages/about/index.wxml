<!--pages/protoShow/index.wxml-->
<view
  class="fixed left-0 top-0 w-100vw h-100vh bg-hex-eef2f7"
  style="z-index: -1"
></view>
<!-- 标题 -->
<nav-bar
  title="关于"
  left-arrow
  bind:leftTap="goBack"
  bind:longpress="titlePress"
></nav-bar>
<view class="h-24rpx"> </view>
<van-cell-group inset>
  <van-cell
    wx:for="{{list}}"
    wx:key="value"
    title="{{item.title}}"
    is-link
    data-value="{{item.value}}"
    bind:click="handleTap"
  ></van-cell>

  <block wx:if="{{envVersion !== 'release'}}">
    <van-cell
      title="调试工具【{{curEnv}}】"
      is-link
      center
      bind:click="toDebugUtil"
    />
  </block>

  <van-cell
    title="版本号"
    value="v{{version}}"
    center
    border="{{false}}"
  />
  <van-cell
    wx:if="{{envVersion !== 'release' && releaseTime}}"
    title="发布时间"
    value="{{releaseTime}}"
    center
    border="{{false}}"
  />

  <van-cell
    wx:if="{{isdebug}}"
    title="子设备产测"
    is-link
    center
    bind:click="toDebugUtil"
  />
</van-cell-group>
