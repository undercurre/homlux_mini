<!--package-room/index/index.wxml-->
<!-- 背景 -->
<svg-icon name="room-bg" width="100vw" height="100vh" class="fixed bg-z-index"></svg-icon>
<!-- 标题 -->
<nav-bar title="{{title}}" left-arrow bind:leftTap="back"></nav-bar>

<!-- 页面主体 -->
<view class="w-full flex flex-col items-center">
  <!-- 场景 -->
  <view class="scene-card-bg w-686rpx h-96rpx flex items-center rounded-24rpx mt-24rpx">
    <image src="/assets/img/base/scene.png" class="w-64rpx h-64rpx ml-24rpx"></image>
    <text class="text-32rpx text-hex-333333 font-medium ml-16rpx">场景</text>
    <view class="flex flex-1 justify-end">
      <van-button custom-class="fav-btn" type="default">收藏</van-button>
    </view>
  </view>
</view>
