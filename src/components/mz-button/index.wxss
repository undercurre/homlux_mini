.img,
.imgActive,
.round,
.square {
  position: absolute;
  width: 100%;
  height: 100%;
  filter: drop-shadow(0px 2px 18px rgba(57, 57, 57, 0.06));
}

.round {
  border-radius: 100%;
  background-image: linear-gradient(198deg, #f2f5fc 0%, #ffffff 56%);
}

.square {
  border-radius: 32rpx;
  background-image: linear-gradient(198deg, #f2f5fc 0%, #ffffff 56%);
}
