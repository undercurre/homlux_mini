.log-bg {
  border-radius: 32rpx;
  background-color: #f7f8f9;
}

.log-item {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
  padding: 18rpx 48rpx;
  width: 100%;
  height: 88rpx;
  margin-top: 32rpx;
}

.log-step {
  position: absolute;
  top: 50rpx;
  left: 180rpx;
  height: 110rpx;
  width: 2rpx;
  background-color: #e5e5e8;
}

.circle {
  position: absolute;
  left: 180rpx;
  margin-left: -5rpx;
  width: 12rpx;
  height: 12rpx;
  border-radius: 12rpx;
  box-sizing: border-box;
  background-color: #c7c7cc;
}
