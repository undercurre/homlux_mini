<view class="w-750rpx flex flex-col">
  <view class="flex">
    <text class="ml-48rpx text-black-59 font-normal text-28rpx">功能设置</text>
  </view>
  <view>
    <van-cell-group inset>
      <van-cell
        title="电子反锁"
        bind:click="toChangeWifi"
      >
        <van-switch
          slot="right-icon"
          checked="{{electronicLock}}"
          bind:change="changeElectronicLock"
        />
      </van-cell>

      <van-cell
        title="省电模式"
        bind:click="toggleActionSheet"
      >
        <van-switch
          slot="right-icon"
          checked="{{powerSaving}}"
          bind:change="changePowerSaving"
        />
      </van-cell>
      <van-cell
        title="异常提醒"
        bind:tap="toPage"
        data-url="/package-mine/device-manage/abnormal/index"
        is-link
      >
      </van-cell>

      <van-cell
        title="隐私协议"
        bind:tap="toPage"
        data-url="/package-mine/device-manage/abnormal/index"
        is-link
        border="{{false}}"
      >
      </van-cell>
    </van-cell-group>
  </view>
</view>

<van-toast id="van-toast" />
