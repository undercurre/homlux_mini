<!-- 标题 -->
<nav-bar
  title="权限管理"
  left-arrow
  show-go-home
  bind:leftTap="goBack"
></nav-bar>

<view
  class="fixed left-0 top-0 w-100vw h-100vh bg-hex-eef2f7"
  style="z-index: -1"
></view>

<view class="h-100vh flex flex-col items-center">
  <view class="tabs-wrapper mt-40rpx">
    <mz-tabs
      tabs="{{typeTabs}}"
      activeId="{{ 1 }}"
      itemWidth="164"
      itemHeight="72"
      textColorActive="#488FFF"
      bind:change="onRoomChange"
    >
    </mz-tabs>
  </view>

  <view
    class="flex-1"
    id="content"
  >
    <view class="mt-24rpx w-750rpx">
      <mz-cell-group inset>
        <mz-cell
          bind:click=""
          title="用户一"
          is-link
        >
        </mz-cell>
        <mz-cell
          bind:click=""
          title="用户二"
          is-link
        >
        </mz-cell>
        <mz-cell
          bind:click=""
          title="用户一"
          is-link
          border="{{false}}"
        >
        </mz-cell>
      </mz-cell-group>
    </view>
  </view>
</view>
